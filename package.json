{
  "name": "jay-perry-works-site",
  "repository": "https://github.com/jayperryworks/jayperryworks-site/",
  "description": "The website for Jay Perry Works",
  "version": "3.1.0",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "preexport": "node ./scripts/sitemap && node ./scripts/redirects",
    "export": "sapper export --legacy --timeout 20000",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "axios": "^0.19.2",
    "bourbon": "^6.0.0",
    "compression": "^1.7.1",
    "date-fns": "^2.12.0",
    "fs": "0.0.1-security",
    "include-media": "^1.4.9",
    "js-yaml": "^3.13.1",
    "markdown-it": "^10.0.0",
    "markdown-it-footnote": "^3.0.2",
    "minimist": ">=1.2.3",
    "modularscale-sass": "^3.0.10",
    "normalize-css": "^2.3.1",
    "path": "^0.12.7",
    "polka": "^1.0.0-next.11",
    "sharp": "^0.25.2",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "npm-run-all": "^4.1.5",
    "prettify-xml": "^1.2.0",
    "rollup": "^1.32.1",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-svgo": "^1.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.12",
    "sass": "^1.26.5",
    "svelte": "^3.21.0",
    "svelte-preprocess": "^3.7.4"
  }
}
