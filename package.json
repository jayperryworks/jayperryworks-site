{
		"name": "jay-perry-works-site",
		"repository": "https://github.com/jayperryworks/jayperryworks-site/",
		"description": "The website for Jay Perry Works",
		"version": "3.1.0",
		"scripts": {
				"prep:sitemap": "node ./scripts/sitemap",
				"prep:redirects": "node ./scripts/redirects",
				"prep:images": "node ./scripts/optimizeImages.js",
				"prep": "run-s prep:*",
				"css:build": "node ./scripts/css/index.js",
				"css:prettier": "prettier --write ./static/stylesheets/*",
				"css:dev": "run-s css:build css:prettier",
				"dev": "sapper dev",
				"build": "sapper build --legacy",
				"preexport": "npm run prep",
				"export": "sapper export --legacy",
				"start": "node __sapper__/build",
				"cy:run": "cypress run",
				"cy:open": "cypress open",
				"test": "run-p --race dev cy:run"
		},
		"dependencies": {
				"autoprefixer": "^9.8.6",
				"axios": "^0.21.1",
				"bourbon": "^6.0.0",
				"compression": "^1.7.1",
				"date-fns": "^2.17.0",
				"fs": "0.0.1-security",
				"include-media": "^1.4.9",
				"js-yaml": "^3.14.1",
				"markdown-it": "^10.0.0",
				"markdown-it-footnote": "^3.0.2",
				"minimist": ">=1.2.3",
				"modularscale-sass": "^3.0.10",
				"normalize-css": "^2.3.1",
				"path": "^0.12.7",
				"polka": "^1.0.0-next.11",
				"sharp": "^0.25.4",
				"sirv": "^0.4.6"
		},
		"devDependencies": {
				"@babel/core": "^7.13.1",
				"@babel/plugin-syntax-dynamic-import": "^7.7.4",
				"@babel/plugin-transform-runtime": "^7.13.7",
				"@babel/preset-env": "^7.13.5",
				"@babel/runtime": "^7.13.7",
				"modularscale-js": "^3.0.1",
				"npm-run-all": "^4.1.5",
				"prettier": "2.2.1",
				"prettify-xml": "^1.2.0",
				"rollup": "^2.42.0",
				"rollup-plugin-alias": "^2.2.0",
				"rollup-plugin-babel": "^4.4.0",
				"rollup-plugin-commonjs": "^10.0.0",
				"rollup-plugin-node-resolve": "^5.2.0",
				"rollup-plugin-replace": "^2.0.0",
				"rollup-plugin-svelte": "^5.2.3",
				"rollup-plugin-svg": "^2.0.0",
				"rollup-plugin-svgo": "^1.1.0",
				"rollup-plugin-terser": "^7.0.2",
				"sapper": "^0.29.1",
				"sass": "^1.32.8",
				"serialize-javascript": ">=3.1.0",
				"svelte": "^3.32.3",
				"svelte-preprocess": "^3.9.12"
		}
}
