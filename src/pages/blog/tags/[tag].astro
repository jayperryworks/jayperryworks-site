const tags = await prismicClient.getTags();
const postsByTag = await prismicClient.getAllBySomeTags(tags, {
	predicates: [
		prismic.predicate.at('document.type', 'blog_post'),
	],
	orderings: {
		field: 'my.blog_post.date',
		direction: 'desc',
	},
});

