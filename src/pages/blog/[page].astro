import prismic from '@lib/prismic.ts';

export async function getStaticPaths({ paginate }) {
	const response = await prismic.getAllByType('blog_post');
	const posts = await response;

	return paginate(posts, { pageSize: 10 });
}

const {
	page
} = Astro.props;

